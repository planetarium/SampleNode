{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Kestrel": {
        "EndpointDefaults": {
            "Protocols": "Http2"
        }
    },
    "Solo": {
        "IsEnabled": true,
        "BlockInterval": 8000
    },
    "Action": {
        "ActionLoaderType": "SampleAction.ActionLoader",
        "ModulePath": "<absolute-path-to-the-dll>/SampleAction.dll"
    }
}
